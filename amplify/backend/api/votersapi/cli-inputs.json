{
  "version": 1,
  "paths": {
    "/voters/{voter}": {
      "name": "/voters/{voter}",
      "lambdaFunction": "getVoters",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    },
    "/votes/{voter}": {
      "name": "/votes/{voter}",
      "lambdaFunction": "checkVoter",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    },
    "/keys/{phone}": {
      "name": "/keys/{phone}",
      "lambdaFunction": "sendKey",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read"
        ]
      }
    }
  }
}